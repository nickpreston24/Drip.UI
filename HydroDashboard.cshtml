@model Drip.UI.HydroDashboard

@{
    string dashboard_left_id = $"{Model.Name}_dashboard_left";
    string dashboard_id = $"{Model.Name}_dashboard";

    var slot = @Model.Slot();
    var left = @Model.Slot("left");
}

<section class="grid grid-cols-4 w-screen h-screen bg-neutral-100">

    <div id="@dashboard_left_id">
        @left
    </div>

    <div id="@dashboard_id" class="border-2 col-span-3">
        @slot
    </div>
</section>
